<%@ tag language="java" pageEncoding="UTF-8"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<%@ attribute name="mphoto" %>
<%@ attribute name="mid" %>

<div id="review_mphoto" style=" display:inline-block; border-radius: 70%; width:30px; height:30px; overflow: hidden;">
	<c:if test="${member.mphoto==null}">
		<a href="myReviewBoard.do?mid=${mid}"><img alt="이미지" src="data:image/jpeg;base64," style="width:100%; height:100%; object-fit:cover;"></a>
	</c:if>
	<c:if test="${member.mphoto!=null}">
		<a href="myReviewBoard.do?mid=${mid}"><img alt="이미지" src="./images/${mphoto}"></a>
	</c:if>
</div>
