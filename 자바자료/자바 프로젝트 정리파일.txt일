AbstractRepository		AbstractRepository(Class<T> vo)
						save(T vo) :int
						update(T vo) :int
						deleteById(int no) :int
						selectFindById(int no) :List<T>
						selectAll() :List<T>
						deleteByQuery(String query) :int
						insertByQuery(String query) :int
						updateByQuery(String query) :int
						selectByQuery(String query) :List<T>
						getValue(String fieldName, T vo) :Object
						setValue(ResultSet rs) :T
						setOneValues(Map<String, Object> data) :Map<String, String>
						setTwoValues(Map<String, Object> data) :String
						convertCamelToSnake(String str) :static String
						convertSnakeToCamel(String str) :static String
						
