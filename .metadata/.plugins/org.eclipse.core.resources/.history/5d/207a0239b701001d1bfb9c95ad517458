package kr.co.tj;

import java.util.Scanner;

public class Quiz12 {

	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		
		int i =0;
		int j =0;
		
		
		for(i=0;i<=5;i++) {
			for(j=0;j<i+1;j++) {
				System.out.print("*");	
				}
			System.out.println();

			}
		}
	}
