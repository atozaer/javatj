package kr.co.tj;

public class Test09 {

	public static void main(String[] args) {
		int[] arr = {1,2,3,4,5};
		for(int i =0; i<arr.length;i++) {
			System.out.print(arr[i]);
		}
		int[] arr1 = new int[] {1,2,3,4,5};
		for(int i = 0; i<arr1.length;i++) {
			System.out.println(arr1[i]);
		}
		
	}

}
