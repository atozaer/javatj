package kr.co.tj;

import java.util.Scanner;

public class Quiz05 {

	public static void main(String[] args) {
		int num = 0;
		int count=0;
		
		Scanner scan = new Scanner(System.in);
		
		System.out.println("정수를 입력하시오.");
		
		do{
			num = scan.nextInt();
			if(num%2==0)
			count++;
		}while(count==10);
	}

}
